{
  "name": "vscode-ipso",
  "displayName": "Ipso Language",
  "icon": "images/extension-icon.png",
  "description": "Ipso language support",
  "repository": "https://github.com/LightAndLight/ipso/tree/main/vscode-ipso",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ipso",
        "icon": {
          "light": "images/file-icon.png",
          "dark": "images/file-icon.png"
        },
        "aliases": [
          "Ipso",
          "ipso"
        ],
        "extensions": [
          ".ipso"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ipso",
        "scopeName": "source.ipso",
        "path": "./syntaxes/ipso.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "ipso",
        "path": "./snippets/ipso.json"
      }
    ]
  }
}