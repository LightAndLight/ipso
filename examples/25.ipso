class Show a where
  show : a -> String

instance Show Int where
  show = int.show

prettyArray : Show a => Array a -> String
prettyArray arr = 
  "[${
    if int.eq (array.length arr) 0
    then ""
    else 
      array.foldl 
        (\acc el -> "${acc}, ${show el}") 
        (array.index 0 arr)
        (array.slice 1 (array.length arr - 1) arr)
  }]"

main : IO ()
main = print (prettyArray [0, 1, 2, 3, 4])